extends layout

block content
  .row.page-header: .col-lg-12
      h1= pageHeader.title
  .row
    .col-xs-12.col-md-9
      .row
        .col-xs-12.col-sm-6
        div.volUpdContainer
            form(action="#{formAction}", method="post",  role="form",  name="update-volunteer")
                each value,label in volunteer
                        label #{label}:&nbsp &nbsp
                        input(type="text", name="#{label}", value="#{value}")
                input.savebutton(type="submit", value="Save")